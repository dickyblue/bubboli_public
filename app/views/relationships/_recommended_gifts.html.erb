<div class="row">

    <ul class="no-margin-or-bullet">
    <% @gifts_by_user_pref.each do |gift| %>
      <li class="related-gifts">
        <%= link_to image_tag(gift.gift_images.first.image_url(:sm_thumb_pic)), gift_path(gift) %></br>
        <%= link_to gift.name, gift_path(gift) %>
      </li>
    <% end %>
  </ul>

</div>
